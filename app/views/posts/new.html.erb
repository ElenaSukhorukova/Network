<section>
  <% provide :page_title, t('page.new', model: i18n_model_name(@post).downcase) %>
  <% currently_at %>

  <%= render partial: 'shared/go_back_button', locals: { path: account_path(@account) } %>
  <h1 class='md-4'><%= t('page.new', model: i18n_model_name(@post).downcase) %></h1>

  <%= render partial: 'form',  locals: { model: @post, method: :post, 
    submit: t('submit.create', model: i18n_model_name(@post).downcase), place: 'content' } %>
</section>